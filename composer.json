{
    "name": "cubicmushroom/cqrs",
    "description": "Provides CQRS related services",
    "type": "library",
    "require": {
        "php": "^8.4",
        "symfony/messenger": "^7.3",
        "symfony/uid": "^7.3",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "pestphp/pest": "^4.1",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "CubicMushroom\\Cqrs\\": "src/"
        }
    },
  "autoload-dev": {
      "psr-4": {
          "CubicMushroom\\Cqrs\\Tests\\": "tests/"
      }
  },
    "authors": [
        {
            "name": "Toby Griffiths",
            "email": "toby@cubicmushroom.co.uk"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
